{
  "name": "@raisins/stencil-docs-target",
  "description": "A custom docs target for Stencil components. Edit in Raisins.",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Logan Volkers",
  "module": "dist/stencil.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/stencil.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/stencil.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "@stencil/core": "^2.8.1",
    "husky": "^7.0.2",
    "size-limit": "^5.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@raisins/schema": "^0.0.1",
    "split-on-first": "^3.0.0"
  }
}
