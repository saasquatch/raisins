{
  "name": "@raisins/stencil-docs-target",
  "description": "A custom docs target for Stencil components. Edit in Raisins.",
  "version": "0.2.7",
  "license": "MIT",
  "author": "Logan Volkers",
  "module": "dist/stencil-docs-target.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/saasquatch/raisins",
    "directory": "packages/stencil-docs-target"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "cd my-kitchen-sink && npm i && npm run build && cd ../ && tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/stencil.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/stencil.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "@stencil/core": "^2.8.1",
    "@types/jest": "^27.0.2",
    "ajv": "^6",
    "husky": "^7.0.2",
    "size-limit": "^5.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@raisins/schema": "^0.7.0",
    "i": "^0.3.7",
    "npm": "^8.8.0",
    "split-on-first": "^3.0.0"
  },
  "gitHead": "8629058a0437bbea5bfb184c13855f4b58a8480f"
}
